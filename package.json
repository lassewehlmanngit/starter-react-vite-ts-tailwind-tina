{
  "name": "marketing-site-starter-tina-vite",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "tinacms dev -c \"vite\"",
    "dev:vite": "vite",
    "build": "npm run generate:sitemap && npm run generate:robots && npm run tina:build && vite build && npm run postbuild",
    "build:local": "npm run generate:sitemap && npm run generate:robots && vite build && npm run postbuild",
    "tina:build": "tinacms build || echo 'Tina build skipped (no credentials)'",
    "postbuild": "npm run copy:content && npm run generate:static-paths",
    "copy:content": "cp -r content dist/content",
    "preview": "vite preview",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "generate:sitemap": "tsx scripts/generate-sitemap.ts",
    "generate:robots": "tsx scripts/generate-robots.ts",
    "generate:static-paths": "tsx scripts/generate-static-paths.ts"
  },
  "dependencies": {
    "@fontsource/inter": "^5.0.0",
    "@sentry/react": "^8.0.0",
    "clsx": "^2.1.1",
    "front-matter": "^4.0.2",
    "i18next": "^25.7.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "lucide-react": "^0.555.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^16.4.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "6.22.3",
    "rehype-raw": "^7.0.0",
    "tailwind-merge": "^3.4.0",
    "tinacms": "^2.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@playwright/test": "^1.40.0",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@tinacms/cli": "^2.0.0",
    "@types/node": "^22.14.0",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.0.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^23.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.2.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^7.0.0",
    "vite": "^6.2.0",
    "vite-imagetools": "^7.0.0",
    "vitest": "^1.0.4"
  }
}
